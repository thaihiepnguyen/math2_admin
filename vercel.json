{
    "builds": [
        {
            "src": "math2/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11.4"
            }
        }
    ],
    "routes": [
        { "src": "/static/(.*)", "dest": "/static/$1" },
        {
            "src": "/(.*)",
            "dest": "math2/wsgi.py"
        }
    ]
}